		TNT1 A 0 A_GiveInventory("RawEnergy")
        TNT1 A 0 ACS_ExecuteWithResult(594, 20, 3, 1)  // pickup message, never do pause
        TNT1 A 0 A_SetBlend("Red", 0.5, 16)
        stop
    }
}


