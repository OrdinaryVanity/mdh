        RTNK ABCD 3 BRIGHT
		RTNK EFGH 3 BRIGHT
        loop

      Pickup:
        //TNT1 A 0 A_JumpIfInventory("ETNotInThisLevel",1,"PickupFail")
        TNT1 A 0 A_JumpIfInventory("EnergyTankAcquired",20,"PickupFailCheck")

        TNT1 A 0 ACS_ExecuteWithResult(594, 20, 3)  // pickup message
