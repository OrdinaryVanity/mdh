actor PowerBombShockwave
{
    Projectile
	RipperLevel 11
    +RIPPER
    +BLOODLESSIMPACT
    +NOINTERACTION
    +PIERCEARMOR
    +EXTREMEDEATH
    +FORCERADIUSDMG
	+FORCEXYBILLBOARD
    +SERVERSIDEONLY

    Radius 2
    Height 2
    DamageType "PowerBomb"

    Speed 12

    Obituary "%k vaporized %o."

    States
    {
      Spawn:
        TNT1 B 0
        TNT1 B 0 A_SpawnItemEx("PowerBombShockwaveClientside", 0,0,0, velx,vely,velz, 0, SXF_ABSOLUTEMOMENTUM)
        TNT1 B 0 A_ChangeFlag("NOINTERACTION", 0)
        TNT1 AA 4 bright A_Explode(18, 128, XF_EXPLICITDAMAGETYPE, "PowerBomb") // First half-second //16 damage.
        TNT1 A 0 ACS_ExecuteWithResult(454, 1, 35)
        TNT1 A  4 bright A_Explode(36, 128, XF_EXPLICITDAMAGETYPE, "PowerBomb") //32 damage
        TNT1 AA 3 bright A_Explode(36, 128, XF_EXPLICITDAMAGETYPE, "PowerBomb") //32 damage
        TNT1 A 0 ACS_ExecuteWithResult(454, 1, 35)
        TNT1 AAA 3 bright A_Explode(54, 160, XF_EXPLICITDAMAGETYPE, "PowerBomb") // Second half-second //48 damage
        TNT1 A 0 ACS_ExecuteWithResult(454, 1, 35)
        TNT1 AAA 3 bright A_Explode(54, 160, XF_EXPLICITDAMAGETYPE, "PowerBomb") // Second half-second //damage
        TNT1 A 0 ACS_ExecuteWithResult(454, 1, 35)
        TNT1 AAAAAAAAA 2 bright A_Explode(72, 160, XF_EXPLICITDAMAGETYPE, "PowerBomb") // Third half-second //64 damage
        stop
    }
}

